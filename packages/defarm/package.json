{
  "name": "@undefiorg/defarm",
  "version": "0.4.0",
  "main": "lib/index.js",
  "types": "lib",
  "license": "MIT",
  "author": "katopz",
  "scripts": {
    "gcp-login": "npx google-artifactregistry-auth",
    "test-dev": "jest --clearCache && jest --watch --runInBand --detectOpenHandles --no-cache ",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "clean": "rm -rf lib",
    "coverage": "jest --coverage",
    "version-bump": "git commit -am \"Version bump to $(npm version patch | cut -c 2-)\"",
    "prebuild": "npm run clean && npm test",
    "build": "tsc --project tsconfig.build.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "dev": "tsc -b -w",
    "release": "npm run build && npm publish --access=public"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@defillama/sdk": "^2.2.0",
    "@ethersproject/units": "^5.4.0",
    "@pancakeswap/sdk": "^2.3.2",
    "bignumber.js": "^9.0.1",
    "ethers": "^5.4.4",
    "node-fetch": "^2.6.1",
    "web3": "^1.5.1",
    "web3-core-helpers": "^1.5.1",
    "web3-eth-contract": "^1.5.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.0",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.24.0",
    "firebase-functions-test": "^0.3.1",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "ts-jest": "^27.0.4",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "ethers": "^5.4.4"
  }
}