{
  "name": "@undefiorg/defarm",
  "version": "0.27.1",
  "main": "lib/index.js",
  "types": "lib",
  "license": "MIT",
  "author": "katopz",
  "scripts": {
    "gcp-login": "npx google-artifactregistry-auth",
    "test-dev": "jest --clearCache && jest --watch --runInBand --detectOpenHandles --no-cache ",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "clean": "rm -rf lib",
    "coverage": "jest --coverage",
    "bump-minor": "git commit -am \"Version bump to $(npm version minor | cut -c 2-)\" > /dev/null 2>&1 && git push",
    "bump-patch": "git commit -am \"Version bump to $(npm version patch | cut -c 2-)\" > /dev/null 2>&1 && git push",
    "prebuild": "npm run clean",
    "build": "tsc --project tsconfig.build.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "dev": "tsc -b -w",
    "prerelease": "export VERSION=$(grep '\"version\"' package.json | cut -d '\"' -f 4 | head -n 1) && export GIT_MERGE_AUTOEDIT=no && echo Release $VERSION && git fetch . develop:main && git push --all --follow-tags && unset GIT_MERGE_AUTOEDIT",
    "release": "npm run build && npm publish --access=public"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@defillama/sdk": "^2.2.10",
    "@ethersproject/units": "^5.4.0",
    "@pancakeswap/sdk": "file:./@pancakeswap/sdk",
    "@uniswap/token-lists": "^1.0.0-beta.25",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "bignumber.js": "^9.0.1",
    "dotenv": "^10.0.0",
    "ethers": "^5.4.6",
    "lodash": "^4.17.21",
    "node-fetch": "2",
    "web3": "^1.5.2",
    "web3-core-helpers": "^1.5.2",
    "web3-eth-contract": "^1.5.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node-fetch": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.24.2",
    "firebase-functions-test": "^0.3.2",
    "jest": "^27.1.1",
    "nodemon": "^2.0.12",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3",
    "typescript": "^4.4.2"
  },
  "peerDependencies": {
    "ethers": "^5.4.6"
  }
}
